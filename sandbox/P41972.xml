  <reference evidence="6 7 8" key="2">
    <citation type="journal article" date="1999" name="Science" volume="285" first="1074" last="1077">
      <title>Insights into editing from an Ile-tRNA synthetase structure with tRNAIle and mupirocin.</title>
      <authorList>
        <person name="Silvian L.F."/>
        <person name="Wang J."/>
        <person name="Steitz T.A."/>
      </authorList>
